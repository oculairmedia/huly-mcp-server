# Huly MCP Server Configuration
# Copy this file to .env and fill in your values

# Huly instance configuration
HULY_URL=https://your-huly-instance.com
HULY_EMAIL=your-email@example.com
HULY_PASSWORD=your-password
HULY_WORKSPACE=your-workspace-name

# Server configuration
PORT=3000
NODE_ENV=development

# GitHub token for @hcengineering packages (required for npm install)
GITHUB_TOKEN=your-github-token