# Test Environment Configuration
# Copy this file to .env.test and fill in your test credentials

# Huly Test Instance Configuration
HULY_URL=https://your-test-huly-instance.com
HULY_EMAIL=test-user@example.com
HULY_PASSWORD=your-test-password
HULY_WORKSPACE=test-workspace

# Test-specific settings
NODE_ENV=test
LOG_LEVEL=error
PORT=3456

# GitHub token (if needed for dependencies)
GITHUB_TOKEN=your-github-token

# Note: Use a dedicated test workspace to avoid interfering with production data
# Integration tests will create and delete many resources during testing